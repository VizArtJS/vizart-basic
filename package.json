{
  "name": "vizart-basic",
  "version": "2.0.4",
  "description": "VizArt Basic Charts: bar, row, rose, line, area, etc.",
  "main": "dist/vizart-basic.js",
  "module": "dist/vizart-basic.mjs",
  "jsnext:main": "dist/vizart-basic.mjs",
  "style": "dist/vizart-basic.css",
  "scripts": {
    "_prebuild": "eslint src test",
    "build": "rollup -c",
    "dev": "rollup -c rollup.config.dev.js -w",
    "watch": "rollup -c -w",
    "pretest": "npm run build",
    "test:watch": "mocha --reporter min --watch",
    "test": "mocha",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "prepublish": "npm test",
    "pretty": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,demo,dist}__}/**/*.js\""
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.12",
    "d3-brush": "^1.0.6",
    "d3-chord": "^1.0.6",
    "d3-collection": "^1.0.7",
    "d3-color": "^1.2.3",
    "d3-ease": "^1.0.5",
    "d3-format": "^1.3.2",
    "d3-interpolate": "^1.3.2",
    "d3-quadtree": "^1.0.5",
    "d3-random": "^1.1.2",
    "d3-scale": "^2.1.2",
    "d3-selection": "^1.3.2",
    "d3-shape": "^1.2.2",
    "d3-time": "^1.0.10",
    "d3-time-format": "^2.1.3",
    "d3-timer": "^1.0.9",
    "d3-transition": "^1.1.3",
    "d3-voronoi": "^1.1.4",
    "vizart-core": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^5.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "prettier": "1.15.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-watch": "^4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VizArtJS/vizart-basic.git"
  },
  "keywords": [
    "VizArt",
    "ES6",
    "Data Visualization"
  ],
  "author": "Xing Yun",
  "license": "MIT",
  "homepage": "https://vizartjs.github.io/"
}
